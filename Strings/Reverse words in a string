using namespace std;

int main() {
	
	int t;cin>>t;
	
	while(t--)
	{
	    string a;cin>>a;vector<string>c;
	    
	    for(int i=0;i<a.size();)
	    {string b;
	        while(a[i]!='.' && i<a.size())
	        {
	            b+=a[i];i++;
	        } c.push_back(b);i++;
	    }
	    
	    for(int i=c.size()-1;i>0;i--)
	    cout<<c[i]<<".";
	    cout<<c[0]<<endl;
	}
	
	return 0;
}
